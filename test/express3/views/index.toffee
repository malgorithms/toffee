    <script>
      $(function() {
        $('.test_case').each(function() {
          var el = $(this);
          el.find('.server_output, .script_output').each(function () {
            if ($(this).html() == el.find('.expected_output').html()) {
              $(this).addClass("success").removeClass("fail");
            }
          });
        });
      })
