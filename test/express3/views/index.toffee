<!-- ************ post_process -->
<tr class="test_case">
  <td class="test_cell">post_process</td>
  <td class="expected_output" id="expected_15">This-is-a-hidden-message

.dlrow leurc ,eybdooG
 .dlrow ,olleH</td>
  <td class="server_output fail" id="server_15">#{partial '../../cases/post_process/input.toffee', {
  greeting: 'Hello'
  postProcess: (s) -> (c for c in s by -1).join ''
}}</td>
  <td class="script_output fail" id="browser_15"></td>
</tr>
<script type="text/javascript">
  var script_res = toffee.templates["/post_process/input.toffee"].render(({
  greeting: 'Hello',
  postProcess: function(s) {
    var c;
    return ((function() {
      var i, results;
      results = [];
      for (i = s.length - 1; i >= 0; i += -1) {
        c = s[i];
        results.push(c);
      }
      return results;
    })()).join('');
  }
}));
  $("#browser_15").html(script_res);
</script>






